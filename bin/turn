#!/usr/bin/env ruby
require_relative '../lib/turn'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
display_board(board)

def turn(board)
  counter = 0
  while turn < 9
    puts "Please enter 1-9"
    board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
    counter += 1
  end
end
